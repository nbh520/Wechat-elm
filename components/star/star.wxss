/* components/star/star.wxss */
.star{
  float: left;
  font-size: 0;
}
.star .star-item{
  display: inline-block;
  background-repeat: no-repeat;
  width: 20rpx;
  height: 20rpx;
  margin-right: 10rpx;
  background-size: 20rpx 20rpx;
}
.star .star-item:last-child{
  margin-right: 0
}
.star .on{
  background-image: url('data:image/png;base64,')
}
.star .half{
  background-image: url('data:image/png;base64,')
}
.star .off{
  background-image: url('data:image/png;base64,')
}

